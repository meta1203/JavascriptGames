{
  "name": "PixelTwinShooter",
  "version": "0.0.1",
  "description": "a basic RotMG game written in JS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "watch": "run-p serve watch:*",
    "build": "run-p build:*",
    "build:js": "webpack --mode=production",
    "watch:js": "onchange \"js\" -- webpack --mode=development",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange \"./\" -- npm run build:html"
  },
  "author": "Matt et al.",
  "license": "none",
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "eslint": "^7.18.0",
    "eslint-loader": "^4.0.2",
    "htmlnano": "^0.2.8",
    "onchange": "^7.1.0",
    "posthtml": "^0.15.1",
    "posthtml-cli": "^0.9.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "npm-run-all": "^4.1.5"
  }
}
